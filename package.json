{
  "name": "Carebnb",
  "version": "1.0.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "engines": {
    "node": "14.15.1",
    "npm": "6.4.1"
  },
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^1.4.1",
    "@stripe/stripe-js": "^1.16.0",
    "axios": "^0.21.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "moment": "^2.29.1",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-calendar": "^3.4.0",
    "react-dates": "^21.8.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-md": "^2.9.0",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-with-direction": "^1.3.1",
    "redux-logger": "^3.0.6",
    "reselect": "^4.0.0",
    "sass": "^1.35.2",
    "stripe": "^6.28.0",
    "styled-components": "^5.3.0",
    "styled-reset": "^4.3.4",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.1",
    "prettier": "^2.3.0"
  }
}
